<div class="container">
  <div class="jumbotron">

    <h4><%= flash[:notice] %></h4>

    <h1>All Artists</h1>
    <% @artists.each do |artist| %>
    <h3><%= link_to artist.name, artist_path(artist.id) %></h3>
    <% end %>
  </div>
  <nav class="navbar navbar-default text-center">
    <h4><%= link_to "New Artist", new_artist_path %> - <%= link_to "New Playlist", new_playlist_path %></h4>
  </nav>
</div>
